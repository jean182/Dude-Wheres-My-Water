{
  "globals": {
    "__DEV__": true
  },
  "extends": [
    "airbnb",
    "prettier",
    "prettier/react",
    "plugin:prettier/recommended",
    "eslint-config-prettier"
  ],
  "env": {
    "jest": true
  },
  "parser": "babel-eslint",
  "rules": {
    "import/no-unresolved": "off",
    "react/prop-types": [2, { "ignore": ["children"] }],
    "react/jsx-filename-extension": [
      1,
      {
        "extensions": [".js", ".jsx"]
      }
    ],
    "prettier/prettier": [
      "error",
      {
        "trailingComma": "es5",
        "singleQuote": true,
        "printWidth": 100
      }
    ],
    "global-require": "off",
    "import/no-extraneous-dependencies": ["error", { "devDependencies": true }]
  },
  "plugins": ["prettier"]
}
